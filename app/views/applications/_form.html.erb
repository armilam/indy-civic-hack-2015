<%= simple_form_for [@student, @application] do |f| %>
  <p>Please complete the application.</p>
  <div class="form-group">
    <%= f.input :wl_building, label: "Building", required: true %>
  </div>
  <div class="form-group">
    <%= f.input :wl_program, label: "Program", required: true %>
  </div>
  <div class="form-actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
